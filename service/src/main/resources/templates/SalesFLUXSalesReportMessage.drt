template header

tname
expression
brid
rulemsg
type
level

package eu.europa.ec.fisheries.uvms.rules.service.business.activity;

import eu.europa.ec.fisheries.uvms.rules.service.business.fact.SalesFLUXSalesReportMessageFact;

template "@{tname}"

rule "Sales FLUX Sales Report Message @{tname} - @{brid}"

when

    $fact : SalesFLUXSalesReportMessageFact(@{expression})

then
    $fact.setOk(false);
    $fact.addWarningOrError("@{type}", "@{rulemsg}", "@{brid}", "@{level}");
end

end template