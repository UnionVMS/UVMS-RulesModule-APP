<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <custom-converters>
            <converter type="eu.europa.ec.fisheries.uvms.rules.service.converter.XMLGC2XMLGC">
                <class-a>javax.xml.datatype.XMLGregorianCalendar</class-a>
                <class-b>javax.xml.datatype.XMLGregorianCalendar</class-b>
            </converter>
        </custom-converters>
    </configuration>

    <mapping map-null="true">
        <class-a>eu.europa.ec.fisheries.schema.rules.movement.v1.RawMovementType</class-a>
        <class-b>eu.europa.ec.fisheries.schema.movement.v1.MovementBaseType</class-b>
        <field>
            <a>mobileTerminal.connectId</a>
            <b>connectId</b>
        </field>
    </mapping>
      
</mappings>
