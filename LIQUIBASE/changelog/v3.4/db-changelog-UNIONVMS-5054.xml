<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">
    <changeSet id="UNIONVMS-5054" author="nomikosi">

        <update tableName="context_expression">
            <column name="expression" value="simpleFAQueryParameterTypeCodes != null &amp;&amp; !hasOnlyOneAssetOfTheGiven(&quot;TRIPID&quot;,&quot;VESSELID&quot;,simpleFAQueryParameterTypeCodes)"/>
            <where>rule_id = '366'</where>
        </update>
        <rollback>
            <update tableName="context_expression">
                <column name="expression" value="simpleFAQueryParameterTypeCodes != null &amp;&amp; simpleFAQueryParameterTypeCodes.size &lt; 2"/>
                <where>rule_id = '366'</where>
            </update>
        </rollback>
    </changeSet>
</databaseChangeLog>