<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="UNIONVMS-4164" author="emrinalgr">

        <update tableName="rule">
            <column name="expression" value="!isEmpty(appliedAAPProcessConversionFactorNumber) &amp;&amp; !isEquals(appliedAAPProcessConversionFactorNumber, 0) || !isGreaterOrEquals(appliedAAPProcessConversionFactorNumber, 1)"/>
            <column name="rule_created_on"/>
            <where>BR_ID = 'FA-L01-00-0174'</where>
        </update>
    </changeSet>

    <changeSet id="UNIONVMS-4166" author="kovian">
        <insert tableName="rule">
            <column name="rule_id" value="452"/>
            <column name="br_id" value="FA-L02-00-0045"/>
            <column name="expression"
                    value="!isEmpty(ownerFluxPartyIds) &amp;&amp; !isEmpty(faReportMessageOwnerFluxPartyIds) &amp;&amp; idTypeValueContainsAny(faReportMessageOwnerFluxPartyIds, &quot;XEU&quot;, &quot;XFA&quot;) &amp;&amp; !idTypesListsAreEqual(ownerFluxPartyIds, faReportMessageOwnerFluxPartyIds)"/>
            <column name="note" value="The party sending must be allowed to send the message"/>
            <column name="message"
                    value="Report Owner ID not consistent with FLUX TL envelope parameters."/>
            <column name="level" value="L03"/>
            <column name="error_type" value="WARNING"/>
            <column name="rule_created_on"/>
            <column name="template_id" value="1"/>
            <column name="property_names" value="ownerFluxPartyIds,faReportMessageOwnerFluxPartyIds"/>
            <column name="disabled" value="false"/>
        </insert>

        <insert tableName="rule">
            <column name="rule_id" value="600"/>
            <column name="br_id" value="FA-L02-00-0600"/>
            <column name="expression" value="codeTypeValueEquals(typeCode, &quot;AREA_ENTRY&quot;) &amp;&amp; (speciesTargetCode == null || (speciesTargetCode.listId != &quot;TARGET_SPECIES_GROUP&quot;))"/>
            <column name="note" value="TBU from MDR"/>
            <column name="message"  value="TBU from MDR"/>
            <column name="level" value="L02"/>
            <column name="error_type" value="ERROR"/>
            <column name="rule_created_on"/>
            <column name="template_id" value="13"/>
            <column name="property_names" value="speciesTargetCode"/>
            <column name="disabled" value="false"/>
        </insert>

        <insert tableName="rule">
            <column name="rule_id" value="601"/>
            <column name="br_id" value="FA-L02-00-0601"/>
            <column name="expression" value="(codeTypeValueEquals(typeCode, &quot;FISHING_OPERATION&quot;) || codeTypeValueEquals(typeCode, &quot;JOINT_FISHING_OPERATION&quot;) || codeTypeValueEquals(typeCode, &quot;AREA_EXIT&quot;))
                         &amp;&amp; (speciesTargetCode == null || (speciesTargetCode.listId != &quot;FAO_SPECIES&quot;))"/>
            <column name="note" value="TBU from MDR"/>
            <column name="message"  value="TBU from MDR"/>
            <column name="level" value="L02"/>
            <column name="error_type" value="ERROR"/>
            <column name="rule_created_on"/>
            <column name="template_id" value="13"/>
            <column name="property_names" value="speciesTargetCode"/>
            <column name="disabled" value="false"/>
        </insert>

        <insert tableName="rule">
            <column name="rule_id" value="605"/>
            <column name="br_id" value="FA-L02-00-0605"/>
            <column name="expression" value="subActivity  &amp;&amp; codeTypeValueEquals(faReportDocumentTypeCode, &quot;DECLARATION&quot;) &amp;&amp; (mainActivityType == &quot;JOINT_FISHING_OPERATION&quot;)
                                            &amp;&amp; atMostOneVTMRoleCodeWithValue(&quot;CATCHING_VESSEL&quot;)"/>
            <column name="note" value="TBU from MDR"/>
            <column name="message"  value="Check value. If the activity is a joint fishing operation declaration, there cannot be more than one occurrence of VesselTransportMeans/RoleCode = 'CATCHING_VESSEL' for the Related- and SpecifiedVesselTransportMeans entities in this FAReportDocument."/>
            <column name="level" value="L02"/>
            <column name="error_type" value="WARNING"/>
            <column name="rule_created_on"/>
            <column name="template_id" value="13"/>
            <column name="property_names" value="relatedVesselTransportMeansRoleCodes"/>
            <column name="disabled" value="false"/>
        </insert>
    </changeSet>

</databaseChangeLog>